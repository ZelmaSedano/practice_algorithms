// create a function that returns elements from certain indexes in an array
function elementsAtIndices(arr1, arr2) {
  // arr1 holds indices, arr2 holds elements
  // create a result variable to return
  let result = [];

  // loop through arr1, returning from arr2[arr1[i]], which is the element from arr2 at arr1's specified index
  for(let i = 0; i < arr1.length; i++) {
    result.push(arr2[arr1[i]]);
  }

  return result;
}

console.log(elementsAtIndices([2,3], [11,22,33,44]));
