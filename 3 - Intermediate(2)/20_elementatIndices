// create a function that has an array of indices, then an array of elements, and then returns the elements from arr2 at the indices in arr1
function elementsAtIndices(arr1, arr2) {
  // create an empty array to return 
  let result = [];

  // loop through arr1, return from arr2[arr[i]]
  for(let i = 0; i < arr1.length; i++) {
    result.push(arr2[arr[i]]);
  }
  return result;
}

console.log(elementsAtIndices([1,2,3], [11,22,33,44,55]));
