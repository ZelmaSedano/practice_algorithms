var doubleConcat = function(nums) {
    // create another array, that's a copy of nums using the spread operator
    let temp = [...nums];
    // concatonate ans onto nums
    let ans = temp.concat(nums);
    return ans;
};

console.log(doubleConcat([1,2,1]); // [1,2,1,1,2,1]
console.log(doubleConcat([1,3,2,1]); // [1,3,2,1,1,3,2,1]
