// create a function that returns true if a two rectangles overlap
function rectangleOverlap(rec1, rec2) {
  // each rec is an array of locations on a graph
  // if the bottom left points of rec2 of smaller than top-right points of rec1
  // if top right points of rec2 are larger than bottom-left points of rec1
  if(rec2[0] < rec1[2] && rec2[1] < rec1[3] && rec2[2] > rec1[0] && rec[3] > rec1[1]) {
    return true;
  } else {
    return false;
  }
}

