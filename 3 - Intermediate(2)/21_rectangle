var isRectangleOverlap = function(rec1, rec2) {
    if(rec2[0] < rec1[2] && rec2[1] < rec1[3] && rec2[2] > rec1[0] && rec2[3] > rec1[1]) {
        return true;
    } else {
        return false;
    }
};

// rec1[0] & rec1[1] = bottom-left corner of 1st rectangle
// rec1[2] & rec2[3] = top-right corner of 1st rectangle

// rec2[0] & rec2[1] = bottom-left corner of 2nd rectangle
// rec2[2] & rec2[3] = top-right corner of 2nd rectangle
