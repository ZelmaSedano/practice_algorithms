function sockMerchant(num, arr){
  // create an empty object to add to
  const socks = {}

  // loop through arr?
  for(let sock of arr) {
    // if we don't have an occurence of arr in obj
    if(!socks[sock]) {
      // this is how you add to obj
      socks[sock] = 1;
    } else {
      // if there already an occurrence of sock in obj, then just add to that occurrence
      // push into object's property as value
      socks[sock] += 1;
    }
  }

  // check to see if value is above 2

  // Option 1:
  // create count counter variable 
  let count = 0;

  // check the value of each property in obj
  for (const value in socks) {
    // if the value is more than or equl to 2
    if (socks[value] >= 2) {
      // we add one pair for every two 
      count = count + Math.floor(socks[value] / 2);
      // count = count + (socks[value] / 2) rounded down
      // count = count + Math.floor(3/2) = 1 
      // 1.5 rounded down
    }
  }
  // once we're out of the loop, return count
  return count;
}

console.log(sockMerchant(9,[10,20,20,10,10,30,50,10,20]));
