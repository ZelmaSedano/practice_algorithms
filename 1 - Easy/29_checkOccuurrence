// create a function that checks to see how many times a given string include a given letter
function stringIncludes(str, char) {
  let counter = 0;

  for (let i = 0; i < str.length; i++) {
    if (str[i] === 'a') {
      counter += 1;
    }
  }

  return counter;
}

console.log(stringIncludes('haa', 'a'));
 
