// create a function that returns true if it makes 10
function makes10(num1, num2) {
  // edge cases? if num1 & num2 are 0, return false; but that's also be true for 1 & 1, so no
  if(num1 + num2 === 10) {
    return true;
  } else if(num1 === 10) {
    return true;
  } else if(num2 === 10) {
    return true;
  } else {
    return false;
  }
}

console.log(makes10(9,10)); // true
console.log(makes10(9,9)); // false
console.log(makes10(1,9)); // true
