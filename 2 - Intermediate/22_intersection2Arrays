// create a function that takes an array of indices, then an array of elements
// only return elements at the indices specified in arr1 from arr2
function fibonacci(num) {
  let sequence = [1,1];

  // loop from 2 to num
  for(let i = 2; i < num; i++) {
    // reset value for sequence[i]
    sequence[i] = sequence[i-1] + sequence[i-2];
    // if num is 3, then 
    // 1st loop: sequence[2] = sequence[1] + sequence[0] = 1
    // 2nd loop: sequence[3] = sequence[2] + sequence[1] = 1 + 1 = 2
  }

  // since sequence is 0-indexed, we return num-1 to return element at num's index
  return sequence[num-1];
}

console.log(fibonacci(8));
